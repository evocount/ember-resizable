{
  "name": "ember-resizable",
  "version": "1.0.1",
  "description": "Resizable component for Ember.",
  "keywords": [
    "ember-addon",
    "resizable",
    "resize"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/evocount/ember-resizable"
  },
  "license": "MIT",
  "author": "Till Theato <till.theato@evocount.de>",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel \"lint:!(fix)\"",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "test": "npm-run-all lint test:*",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each",
    "deploy:demo": "ember build --environment production && ember github-pages:commit --message \"Deploy gh-pages from commit $(git rev-parse HEAD)\" && git push origin gh-pages:gh-pages"
  },
  "dependencies": {
    "ember-cli-babel": "^7.26.6",
    "ember-cli-htmlbars": "^5.7.1",
    "ember-cli-sass": "^10.0.1",
    "ember-decorators": "^6.1.1",
    "ember-lifeline": "^6.0.2",
    "sass": "^1.37.2"
  },
  "devDependencies": {
    "@ember/optional-features": "^2.0.0",
    "@ember/test-helpers": "^2.3.0",
    "@embroider/test-setup": "^0.43.3",
    "@glimmer/component": "^1.0.4",
    "@glimmer/tracking": "^1.0.4",
    "babel-eslint": "^10.1.0",
    "bootstrap": "^5.0.2",
    "broccoli-asset-rev": "^3.0.0",
    "ember-auto-import": "^2.0.2",
    "ember-bootstrap": "^4.9.0",
    "ember-cli": "~3.27.0",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-github-pages": "^0.2.2",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-release-tag": "^0.1.5",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.2",
    "ember-code-snippet": "^3.0.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-load-initializers": "^2.1.2",
    "ember-page-title": "^6.2.2",
    "ember-power-select": "^4.1.6",
    "ember-prism": "^0.10.0",
    "ember-qunit": "^5.1.4",
    "ember-resolver": "^8.0.2",
    "ember-source": "~3.27.5",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^3.5.1",
    "ember-try": "^1.4.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-ember": "^10.5.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-qunit": "^6.2.0",
    "loader.js": "^4.7.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.2",
    "qunit": "^2.16.0",
    "qunit-dom": "^1.6.0"
  },
  "engines": {
    "node": "10.* || >= 12"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "demoURL": "https://evocount.github.io/ember-resizable/"
  }
}
